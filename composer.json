{
    "name": "efficiently/authority-controller",
    "description": "AuthorityController is an PHP authorization library for Laravel 5 which restricts what resources a given user is allowed to access.",
    "keywords": [
        "cancan",
        "can",
        "authority",
        "acl",
        "role",
        "permission",
        "ability",
        "allow",
        "deny",
        "rbac",
        "authorization",
        "authentication",
        "security",
        "laravel",
        "laravel 5",
        "rails"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "TortueTorche",
            "email": "tortuetorche@spam.me"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/stayallive/authority"
        },
        {
            "type": "vcs",
            "url": "https://github.com/stayallive/authority-l5"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "stayallive/authority-l5": "~2.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "mockery/mockery": "0.9.*",
        "orchestra/testbench": "3.0.*",
        "illuminate/html": "5.0.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "tests/AcTestCase.php", "tests/helpers", "tests/fixtures/",
            "tests/fixtures/models", "tests/fixtures/controllers"
        ],
        "files": [
            "src/Efficiently/AuthorityController/helpers.php"
        ],
        "psr-0": {
            "Efficiently\\AuthorityController": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-1.0": "1.0.x-dev",
            "dev-1.1": "1.1.x-dev",
            "dev-1.2": "1.2.x-dev",
            "dev-2.0": "2.0.x-dev"
       }
    },
	"config": {
		"preferred-install": "dist"
	},
	"prefer-stable": true,
  "minimum-stability": "dev"
}
